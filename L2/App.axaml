    <Style Selector="ListBox.FixMargin ListBoxItem">
      <Setter Property="Padding" Value="0"/>
    </Style>
    <Style Selector="ListBox.FixMargin ListBoxItem:nth-child(1), ListBox.FixMargin ListBoxItem:nth-last-child(1)">
